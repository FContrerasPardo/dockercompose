<!DOCTYPE html>
<html lang="en">
<head>
  <!-- Head tags here -->
</head>
<body>
  <h1>Lista de Estudiantes</h1>
  <a href="/agregar">Agregar Estudiante</a>
  <ul>
    <% estudiantes.forEach(estudiante => { %>
      <li>
        <%= estudiante.nombre %> - <%= estudiante.grado %>
        <a href="/estudiantes/editar/<%= estudiante._id %>">Editar</a>
        <form action="/estudiantes/eliminar/<%= estudiante._id %>" method="post">
          <button type="submit">Eliminar</button>
        </form>
      </li>
    <% }); %>
  </ul>
</body>
</html>
